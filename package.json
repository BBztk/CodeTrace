{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "code-trace",
  "displayName": "CodeTrace ",
  "description": "记录代码编辑过程并支持回放，就像延时摄影一样。",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode":"^1.109.5"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "CodeTrace .startRecording",
        "title": "CodeTrace : Start Recording"
      },
      {
        "command": "CodeTrace .stopRecording",
        "title": "CodeTrace : Stop Recording"
      },
      {
        "command": "CodeTrace .playback",
        "title": "CodeTrace : Playback"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.3"
  }
}
